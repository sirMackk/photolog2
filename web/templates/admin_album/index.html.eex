<table>
  <thead>
    <tr>
      <td>Actions</td>
      <td>Name</td>
      <td>Description</td>
      <td>Status</td>
      <td>User</td>
      <td>Created at</td>
    </tr>
  </thead>
  <tbody>
    <%= for album <- @albums do %>
      <tr>
        <td>
          <%= link "edit", to: admin_album_path(@conn, :edit, album) %>
          <%= link "delete", to: admin_album_path(@conn, :get_delete, album) %>
        </td>
        <td><%= album.name %></td>
        <td><%= album.description %></td>
        <td><%= album.status %></td>
        <td><%= album.user.username %></td>
        <td><%= album.inserted_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
